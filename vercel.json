{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.php",
      "use": "vercel-php@0.7.1"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://your-app.vercel.app",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views",
    "SESSION_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "CACHE_DRIVER": "array",
    "CACHE_PATH": "/tmp/storage/framework/cache",
    "SESSION_LIFETIME": "120",
    "FILESYSTEM_DISK": "local",
    "STORAGE_PATH": "/tmp/storage"
  }
}